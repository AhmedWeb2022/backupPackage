{
  "name": "waheed/googledrive-backup",
  "description": "Google Drive adapter and backup integration for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Your Name",
      "email": "your.email@example.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "google/apiclient": "^2.0",
    "league/flysystem": "^3.0",
    "spatie/laravel-backup": "^9.0",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "backup\\GoogleDriveBackup\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "backup\\GoogleDriveBackup\\GoogleDriveBackupServiceProvider"
      ],
      "aliases": {
        "GoogleDriveAdapter": "backup\\GoogleDriveBackup\\Facades\\GoogleDriveAdapter"
      }
    }
  }
}
